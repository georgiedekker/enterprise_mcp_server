# ============================================================================
# enterprise_mcp_server - Secrets Configuration
# ============================================================================
# Copy this to .env and fill in your actual values
# DO NOT commit .env to version control!

# PostgreSQL Password
POSTGRES_PASSWORD=your_postgres_password_here

# JWT Secret Key (at least 32 characters)
JWT_SECRET_KEY=your_jwt_secret_key_at_least_32_chars_long

# Admin User Password
ADMIN_PASSWORD=your_admin_password_here

# Default Admin Password Hash (bcrypt hash)
DEFAULT_ADMIN_PASSWORD_HASH='your_bcrypt_password_hash_here'

# Anthropic API Key
ANTHROPIC_API_KEY=sk-ant-api03-your_anthropic_api_key_here
